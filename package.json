{
  "name": "lexermal_spotify_podcast_bot",
  "version": "0.2.0",
  "description": "A Node.js Telegram Bot to receive notifications for Spotify Podcast empisodes",
  "repository": "https://github.com/lexermal/spotify-podcast-notification-bott",
  "author": "Lexermal",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.3",
    "logform": "2.4.2",
    "spotify-web-api-node": "^5.0.2",
    "sqlite3": "5.0.11",
    "telegraf": "4.8.6",
    "typeorm": "0.3.7",
    "typescript": "4.7.4",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "18.7.6",
    "@types/spotify-web-api-node": "^5.0.11",
    "ts-node": "10.9.1"
  },
  "scripts": {
    "dev": "yarn install && export FETCHING_DURATION=0.9 && export SENDING_DURATION=0.3 && yarn prod",
    "prod": "yarn ts-node --require dotenv/config ./src/index.ts"
  },
  "keywords": [
    "Spotify",
    "Podcast",
    "Telegram",
    "Bot"
  ]
}
